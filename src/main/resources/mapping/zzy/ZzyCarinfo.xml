<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.accp.dao.zzy.IZzyCarinfoDao">
	<resultMap id="BaseResultMap" type="com.accp.vo.zzy.ZzyCarinfoVo">
		<id column="carno" property="carno" jdbcType="VARCHAR" />
		<result column="carxinghao" property="carxinghao" jdbcType="VARCHAR" />
		<result column="bzid" property="bzid" jdbcType="INTEGER" />
		<result column="carpp" property="carpp" jdbcType="VARCHAR" />
		<result column="bzname" property="bzname" jdbcType="VARCHAR" />
		<result column="licheng" property="licheng" jdbcType="INTEGER" />
	</resultMap>
	<select id="queryCarinfo" resultMap="BaseResultMap">
		SELECT `carpp`,`carno`,`carxinghao`,`licheng`,bzname 
		FROM carinfo c INNER JOIN banzu b 
		WHERE c.`bzid`=b.`bzid`	
	</select>
</mapper>